{
  "name": "@veksa/logger",
  "version": "1.0.8",
  "description": "Lightweight, configurable logging library for JavaScript applications",
  "homepage": "https://github.com/veksa/logger",
  "packageManager": "yarn@4.9.2",
  "author": {
    "name": "Aleksandr Khizhnyi",
    "email": "mendler88@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:veksa/logger.git"
  },
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "logger",
    "logging",
    "typescript",
    "console",
    "debug",
    "browser-logging",
    "timestamps"
  ],
  "scripts": {
    "clean": "rimraf lib es dist",
    "build": "yarn clean && tsup",
    "compile": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
